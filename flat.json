{
  "name": "TerminalSize",
  "version": "0.1.0",
  "description": "Get Terminal Size Dimensions",
  "author": "Braden Steffaniak",
  "license": "MIT",
  "defaultTarget": "es6",
  "scripts": {
    "test": "node dist/es6/test.js"
  },
  "sources": {
    "main": {
      "mainClass": "flat/terminalsize/TerminalSize",
      "source": "src",
      "dependencies": {
        "https://github.com/FlatLang/System": "master",
        "https://github.com/FlatLang/IO": "master",
        "https://github.com/FlatLang/Stream": "master",
        "https://github.com/FlatLang/Logger": "master"
      }
    },
    "cmd": {
      "mainClass": "flat/terminalsize/TerminalSize_Command",
      "source": "src",
      "bin": "terminal-size",
      "dependencies": {}
    },
    "test": {
      "mainClass": "flat/terminalsize/TerminalSize_TestSuite",
      "source": "src/**/*_Test*.flat",
      "dependencies": {
        "https://github.com/FlatLang/Test": "master"
      }
    }
  }
}
